 					"g_led","","","","","","","",
 					"","","","","","","","",
 					"","","","","","","","",
@@ -441,7 +441,7 @@ sgpio2: sgpio@102000 {
 					"","","","","","","","",
 					"","","","","","","","",
 					"","","","","","","","",
-					"","","PS_PWROK","POST_COMPLETE","POWER_BUTTON","RESET_BUTTON","","",
+					"","","PS_PWROK","POST_COMPLETE","POWER_BUTTON","RESET_BUTTON","NMI_BUTTON","",
